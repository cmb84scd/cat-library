{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "typescript": "./tsconfig.json"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "root": true,
  "rules": {
    "@typescript-eslint/consistent-type-assertions": [
      "warn",
      { "assertionStyle": "as", "objectLiteralTypeAssertions": "never" }
    ]
  }
}
